<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.deniskorotchenko.mapsp.Last">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/resultTime"
        android:layout_centerHorizontal="true"
        android:text="Поздравляем!\n Вы прошли все задания!\n Нажмите для начала нового квеста."
        android:textAlignment="center" />

    <TextView
        android:id="@+id/resultTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tostart"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"></TextView>




    <Button
        android:id="@+id/tostart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="К началу"
        android:textSize="18dp" />
</RelativeLayout>
